{plugins, [
  {rebar3_github_cache,
   {git, "git://github.com/yurrriq/rebar3_github_cache.git",
    {branch, "master"}}},
  {'lfe-compile', {github, "lfe-rebar3/compile", {tag, "0.4.0"}}}
]}.

{provider_hooks, [{pre, [{compile, {lfe, compile}}]}]}.

{deps, [
  {lfe,       {github, "rvirding/lfe",           {branch, "develop"}}},
  {clj,       {github, "lfex/clj",               {tag,    "0.4.1"}}},
  {exemplar,  {github, "lfex/exemplar",          {tag,    "0.4.1"}}},
  {levaindoc, {github, "quasiquoting/levaindoc", {tag,    "0.5.1"}}},
  {jsx,       {github, "talentdeficit/jsx",      {tag,    "2.8.0"}}}
]}.

{overrides, [
  {override, 'lfe-compile', [
    {deps, [
      {lfe, {github, "rvirding/lfe", {branch, "develop"}}}
    ]}
  ]},
  {override, clj, [
    {deps, [
      {kla, {github, "lfex/kla", {tag, "0.6.1"}}}
    ]}
  ]},
  {override, kla, [
    {deps, [
      {'lfe-version', {github, "lfe-rebar3/version", {tag, "0.3.2"}}}
    ]}
  ]},
  {override, exemplar, [
    {deps, [
      {lutil, {github, "lfex/lutil", {tag, "0.8.1"}}}
    ]}
  ]}
]}.

{profiles, [
  {test, [
    {eunit_compile_opts, [{src_dirs, ["test"]}]},
    {deps, [{ltest, {github, "lfex/ltest", {tag, "0.8.2"}}}]}
  ]}
]}.
